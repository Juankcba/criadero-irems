generator client {
  provider = "prisma-client-js"
  output   = "../prisma/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model blog {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  content String
  createAt DateTime @default(now())
  image String
}
